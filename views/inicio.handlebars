<div id="contacto" class="contactoSection">
    <div class="container text-center">
        <div class="row">
            <div class="col-md-6">
                <section class="formdiv googleSubTitulo">
                    <div class="formularioContacto container">
                        <div class="row">
                            <div class="col-md">
                                <section class="card theForm">
                                    <h5 class="card-header contactHeader">Reservar fechas</h5>
                                    <section class="card-body">
                                        <form action="/ajax/email" method="POST"
                                            onsubmit="return submitEmailForm(this);" id="myForm">
                                            <section class="form-group">
                                                <label for="name">Nombre Completo</label>
                                                <input type="name" name="name" class="form-control formInput"
                                                    id="completeName" aria-describedby="namelHelp" required>
                                            </section>
                                            <section class="form-group">
                                                <label for="exampleInputEmail1">Correo Electrónico</label>
                                                <input type="email" name="email" class="form-control formInput"
                                                    id="inputEmail1" aria-describedby="emailHelp" required>
                                                <small id="emailHelp" class="form-text text-muted">Nunca compartiremos
                                                    tu correo
                                                    electrónico con
                                                    terceros.</small>
                                            </section>
                                            <section class="form-group">
                                                <label for="name">Número de contacto</label>
                                                <input type="text" name="number" class="form-control formInput"
                                                    id="completeName" aria-describedby="namelHelp" required>
                                            </section>
                                            <section>
                                                <div class="form-group">
                                                    <!-- Date input -->
                                                    <label class="control-label" for="date">Día de llegada</label>
                                                    <input class="form-control formInput" id="date" name="date"
                                                        placeholder="MM/DD/YYY" type="text" required/>
                                                </div>
                                            </section>
                                            <section>
                                                <div class="form-group">
                                                    <!-- Date input -->
                                                    <label class="control-label" for="date">Número de noches</label>
                                                    <input class="form-control formInput" id="secondate" name="secondate"
                                                         type="text" required/>
                                                </div>
                                            </section>
                                            <br>
                                            <button
                                                class="btn btn-warning btn-lg submitFormBtn pointer">Enviar</button>
                                            <br>
                                            <br>
                                            <section class="form-check">
                                                <input type="checkbox" class="form-check-input" id="exampleCheck2"
                                                    name="checkboxPolicy" required>
                                                <label class="form-check-label submitFormBtn" for="exampleCheck2">
                                                    Acepto <span><a href="/privacy" target="_blank"
                                                            rel="noopener noreferrer">política de
                                                            privacidad</a></span></label>
                                            </section>
                                            <br>

                                        </form>
                                    </section>
                                </section>
                                <!-- <section class="theForm">
                    </section> -->
                            </div>
                        </div>

                    </div>

                </section>
            </div>
            <div class="col-md-6 dondeDormiras">
                <hr>
                <h3>¿Dónde dormirás?</h3>
                <br>
                <ul class="list-group">
